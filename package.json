{
	"name": "project-aeon",
	"version": "1.0.0",
	"description": "A fullstack MERN SSR boilerplate.",
	"repository": "https://github.com/mattcarlotta/project-aeon",
	"author": "Matt Carlotta",
	"license": "MIT",
	"private": false,
	"sideEffects": false,
	"scripts": {
		"analyze": "rimraf .next/analyze && analyze=true npm run build",
		"build": "inStaging=false next build",
		"checkbuild": "es-check es5 './.next/static/**/*.js'",
		"dev": "NODE_ENV=development next dev",
		"lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .js,.jsx",
		"lint:styles": "stylelint **/*.scss",
		"start": "NODE_ENV=production next start -p 8080",
		"seed": "NODE_ENV=config node ./database/seedDB/index.js",
		"teardown": "NODE_ENV=config node ./database/teardownDB/index.js",
		"test": "NODE_ENV=test jest --config ./jest.json --passWithNoTests",
		"test:cov": "NODE_ENV=test jest --config ./jest.json --coverage",
		"test:watch": "NODE_ENV=test jest --config ./jest.json --watch --onlyChanged",
		"test:watchall": "NODE_ENV=test jest --config ./jest.json --watchAll"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"npm run lint",
			"npm run test"
		],
		"*.scss": [
			"npm run lint:styles"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.1",
		"@material-ui/core": "^4.10.1",
		"axios": "^0.19.2",
		"bcryptjs": "2",
		"bluebird": "^3.7.2",
		"chalk": "^4.1.0",
		"classnames": "^2.2.6",
		"cookie-session": "^1.4.0",
		"dayjs": "^1.8.28",
		"dotenv": "^8.2.0",
		"highlight.js": "^10.0.3",
		"lodash.debounce": "^4.0.8",
		"lodash.get": "^4.4.2",
		"lodash.isempty": "^4.4.0",
		"lodash.random": "^3.2.0",
		"morgan": "^1.10.0",
		"next": "^9.4.4",
		"next-redux-wrapper": "^6.0.2",
		"nprogress": "^0.2.0",
		"omit.js": "^1.0.2",
		"pg-monitor": "^1.4.0",
		"pg-promise": "^10.5.6",
		"rc-select": "^11.0.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-icons": "^3.10.0",
		"react-markdown": "^4.3.1",
		"react-redux": "^7.2.0",
		"react-smde": "^1.0.1",
		"react-toastify": "^6.0.5",
		"redux": "^4.0.5",
		"redux-saga": "^1.1.3",
		"styled-components": "^5.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"address": "^1.1.2",
		"axios-mock-adapter": "^1.18.1",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.0.1",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-react-remove-properties": "^0.3.0",
		"babel-plugin-styled-components": "^1.10.7",
		"browserslist": "^4.12.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^7.2.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-babel-module": "^5.1.2",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.21.1",
		"eslint-plugin-jest": "^23.13.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^4.0.4",
		"eslint-watch": "^7.0.0",
		"extracted-loader": "^1.0.7",
		"file-loader": "^6.0.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"husky": "^4.2.5",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.0.1",
		"jest-css-modules-transform": "^4.0.0",
		"jest-enzyme": "^7.1.2",
