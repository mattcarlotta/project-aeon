		"jest-resolve": "^26.0.1",
		"jest-styled-components": "^7.0.2",
		"jsdom": "^16.2.2",
		"lint-staged": "^10.2.9",
		"node-sass": "^4.14.1",
		"null-loader": "^4.0.0",
		"postcss-loader": "^3.0.0",
		"prettier": "^2.0.5",
		"prop-types": "^15.7.2",
		"react-dev-utils": "^10.2.1",
		"redux-devtools-extension": "^2.13.8",
		"redux-saga-test-plan": "^4.0.0-rc.3",
		"sass-loader": "^8.0.2",
		"stylelint": "^13.6.0",
		"stylelint-config-recommended": "^3.0.0",
